{
    "name": "hyde/devtools",
    "description": "Makes contributing to the HydePHP core framework a bit easier.",
    "keywords": [
        "hyde",
        "devtools"
    ],
    "homepage": "https://github.com/hyde/devtools",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Caen De Silva",
            "email": "caen@desilva.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hyde\\DevTools\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hyde\\DevTools\\DevToolsServiceProvider"
            ]
        }
    }
}
